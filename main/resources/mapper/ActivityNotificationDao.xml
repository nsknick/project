<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.ActivityNotificationDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.ActivityNotificationEntity" id="activitynotificationMap">
        <result property="title" column="title"/>
        <result property="studentid" column="studentid"/>
        <result property="name" column="name"/>
        <result property="phonenumber" column="phonenumber"/>
        <result property="notificationContent" column="notificationContent"/>
        <result property="notificationTime" column="notificationTime"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.ActivityNotificationVO" >
		SELECT * FROM activitynotification  activitynotification
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.ActivityNotificationVO" >
		SELECT  activitynotification.* FROM activitynotification  activitynotification
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.ActivityNotificationView" >

		SELECT  activitynotification.* FROM activitynotification  activitynotification
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.ActivityNotificationView" >
		SELECT * FROM activitynotification  activitynotification <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	

</mapper>
