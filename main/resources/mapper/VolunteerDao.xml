<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dao.VolunteerDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.entity.VolunteerEntity" id="volunteerMap">
        <result property="studentid" column="studentid"/>
        <result property="password" column="password"/>
        <result property="name" column="name"/>
        <result property="gender" column="gender"/>
        <result property="profilepicture" column="profilepicture"/>
        <result property="age" column="age"/>
        <result property="classes" column="classes"/>
        <result property="email" column="email"/>
        <result property="phonenumber" column="phonenumber"/>
        <result property="isReviewed" column="isReviewed"/>
        <result property="reviewResponse" column="reviewResponse"/>
    </resultMap>

	<select id="selectListVO"
		resultType="com.entity.vo.VolunteerVO" >
		SELECT * FROM volunteer  volunteer
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectVO"
		resultType="com.entity.vo.VolunteerVO" >
		SELECT  volunteer.* FROM volunteer  volunteer
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.VolunteerView" >

		SELECT  volunteer.* FROM volunteer  volunteer
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.VolunteerView" >
		SELECT * FROM volunteer  volunteer <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	

</mapper>
